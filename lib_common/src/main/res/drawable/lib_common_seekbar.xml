<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!--定义seekbar滑动条的底色-->
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dp" />
            <gradient
                android:angle="270"
                android:centerColor="@color/lib_common_progress_background"
                android:centerY="0.75"
                android:endColor="@color/lib_common_progress_background"
                android:startColor="@color/lib_common_progress_background" />
        </shape>
    </item>
    <!--定义seekbar滑动条进度颜色-->
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <solid android:color="@color/lib_common_theme_green"/>
            </shape>
        </clip>
    </item>
</layer-list>